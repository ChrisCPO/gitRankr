<div>
  <h3>
  <p> Welcome to gitRankr</p>
  </h3>
  <p> don't have an account sign up here
    <%= link_to "Sign Up", new_user_path %>
  </p>
</div>

<%= form_for :session, url: session_path do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Sign in" %>
  </div>
<% end %>
